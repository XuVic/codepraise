<div class="quality page" id="quality">
    <div class="title-bar">
        <div class="title">
            Quality
        </div>
        <div class="tools">
            <button class="btn btn-dark" id="switch">Quality Problem Distribution</button>
        </div>
    </div>
    <div class="panel">
        <div class="top ">
            <div class="right">
                <div class="a-board board ">
                    <div class="titles ">
                        <span class="title">
                             <%= @panel_view.a_board.title %>
                        </span>
                    </div>
                    <div class="elements ">
                    <% @panel_view.a_board.elements.each do |element| %>
                        <div class="element">
                            <div class="bar-title"> <%= element.title %> </div>
                            <%= element.to_element %>
                        </div>
                    <% end %>
                    </div>
                </div>
            </div>
            <div class="left">
                <div class="b-board board ">
                    <div class="titles ">
                        <span class="title">
                             <%= @panel_view.b_board.title %>
                        </span>
                    </div>
                    <div class="elements ">
                        <% @panel_view.b_board.elements.each do |element| %>
                            <div class="element">
                                <%= element.to_element %>
                            </div>
                        <% end %>
                    </div>
                </div>
            </div>
        </div>
        <div class="bottom ">
            <div class="c-board board ">
                <div class="titles ">
                    <span class="title">
                        <%= @panel_view.c_board.title %>
                    </span>
                </div>
                <div class="elements ">
                    <% @panel_view.c_board.elements.each do |element| %>
                        <div class="element">
                            <%= element.to_element %>
                        </div>
                    <% end %>
                </div>
            </div>
        </div>
    </div>
    <div class="panel_2">
        <div class="d-board board">
            <div class="titles ">
                <span class="title">
                    <%= @panel_view.d_board.title %>
                </span>
            </div>
            <div class="tools">
                <div class="selector">
                    <div class="title">
                        Type
                    </div>
                    <select id="type"></select>
                </div>
                <div class="selector">
                    <div class="title">
                        Contributors
                    </div>
                    <select id="contributor">
                    </select>
                </div>
            </div>
            <div class="elements ">
                <% @panel_view.d_board.elements.each do |element| %>
                <div class="element">
                <%= element.to_element %>
                </div>
                <% end %>
            </div>
        </div>
    </div>
</div>
